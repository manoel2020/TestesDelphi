unit Util;

interface

procedure ExibeMensagem(const fmensagem, ftitulo:String);

implementation

var
   fquantidade:String;

uses
  Vcl.Forms, Winapi.Windows;

procedure ExibeMensagem(const fmensagem, ftitulo:String);
begin
   application.MessageBox(pchar(fmensagem), fTitulo, MB_ICONINFORMATION+MB_OK);
end;

end.
